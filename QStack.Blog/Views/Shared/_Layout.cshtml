<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>@(ViewData["Title"]==null|| "首页".Equals(ViewData["Title"])? "Qy小栈 — 分享技术&生活": ViewData["Title"]+ " — Qy小栈")</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="author" content="Qy" />
    <link rel="shortcut icon" type="image/x-icon" href="~/favicon.ico" />
     <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/css/bootstrap.min.css"
        asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css"
        asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/font-awesome@4.7.0/css/font-awesome.min.css" />
    <environment include="Development">
        <link rel="stylesheet" type="text/css" href="~/css/style.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="~/css/style.min.css" asp-append-version="true" />
    </environment>
    @RenderSection("header", false)
    <environment exclude="Development">
        <script>
            var _hmt = _hmt || [];
            (function () {
                var hm = document.createElement("script");
                hm.src = "https://hm.baidu.com/hm.js?dcafe3a51ab8bcc55a9a224e01f1d260";
                var s = document.getElementsByTagName("script")[0];
                s.parentNode.insertBefore(hm, s);
            })();
        </script>
    </environment>
</head>
<body>
    <div class="header_top_area">
        <div class="container">
            <div class="header_top_inner d-sm-flex justify-content-sm-between">
                <ul class="left_info">
                    <li>Welcome to Qy小栈</li>
                </ul>

            </div>
        </div>
    </div>
    @await Component.InvokeAsync("Navigation")
    @RenderBody()
    <!-- Scroll Top Button -->
    <button class="scroll-top" style="display: none;">
        <i class="fa fa-angle-double-up"></i>
    </button>
    <!-- Preloader -->
    <div class="preloader"></div>
    <footer class="footer_area">
        <div class="copy_right">
            <p>Power by ASP@( System.Runtime.InteropServices.RuntimeInformation.FrameworkDescription)</p>
            <p>Copyright © 2020 qystack.top <a href="http://beian.miit.gov.cn">粤ICP备19150074号-2</a></p>
        </div>
    </footer>
    <!-- Vertically centered scrollable modal -->
    <div class="modal fade" id="bt-modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" style="padding-right: 16px;" aria-modal="true">
        <div id="bt-modal-dialog" class="modal-dialog modal-dialog-centered">
            <div class="modal-content">
                <div class="d-flex justify-content-center">
                    <div class="spinner-border" role="status">
                        <span class="sr-only">Loading...</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.3.1/dist/jquery.min.js"
            asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
            asp-fallback-test="window.jQuery">
    </script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"
            asp-fallback-src="~/lib/popper.min.js">
    </script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/js/bootstrap.min.js"
            asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
            asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal">
    </script>
    <script type="text/javascript" src="~/lib/jquery.nicescroll.min.js"></script>
    <script src="~/vendors/animate-css/wow.min.js"></script>
    <environment include="Development">
    <script type="text/javascript" src="~/js/main.js"></script>
    </environment>
    <environment exclude="Development">
     <script type="text/javascript" src="~/js/main.min.js" asp-append-version="true"></script>
    </environment>
    @RenderSection("Scripts", required: false)
</body>
</html>
